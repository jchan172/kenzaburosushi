<div class="container">
    <div class="row">
    	<img class="centeredimage" src="/assets/ScratchCMS.png" width=200dp>
    	<div class="span4 offset4">
        <%= form_for(:session, url: sessions_path) do |f| %>

          <%= f.label :username %>
          <%= f.text_field :username %>

          <%= f.label :password %>
          <%= f.password_field :password %>

          <br>
          
          <%= f.submit "Log In", class: "btn btn-large btn-inverse" %>
        <% end %>
      </div>
  </div>